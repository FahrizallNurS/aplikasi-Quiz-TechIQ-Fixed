<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <corners android:radius="10dp" />

    <padding
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp"/>

    <gradient
        android:type="radial"
        android:gradientRadius="1000dp"
        android:startColor="#22D3EE"
        android:endColor="#22D3EE" />

    <solid android:color="#FFFF" />

    <stroke
        android:width="2dp"
        android:color="#22D3EE" />
</shape>
